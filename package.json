{
    "name": "boilerplate-monorepo",
    "private": true,
    "workspaces": ["packages/*"],
    "scripts": {
      "dev": "npm --workspace template run dev",
      "build": "npm --workspace template run build",
      "start": "npm --workspace template run start",
      "lint": "npm --workspace template run lint",
      "prettier": "npm --workspace template run prettier",
      "prepack:cli": "npm --workspace fast-boilerplate-next-app run prepack"
    }
  }